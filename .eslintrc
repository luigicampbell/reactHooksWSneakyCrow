{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "printWidth": 100
      }
    ],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-one-expression-per-line": 0,
  },
  "extends": [
    "airbnb",
    "prettier"
  ],
  "settings": {
    "import/resolver": "webpack"
  }
}

